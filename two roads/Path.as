import mx.transitions.Tween;import mx.transitions.easing. *;class Path extends MovieClip{		public var dir:String;	public var mom:MovieClip;		private var arrow : MovieClip;			function onRollOver (){		var twn : Tween = new Tween (this.arrow, "_alpha", Regular.easeInOut, this.arrow._alpha, 100, 0.5, true);	}			function onRollOut (){		var twn : Tween = new Tween (this.arrow, "_alpha", Regular.easeInOut, this.arrow._alpha, 0, 0.5, true);	}		function onRelease (){				this.mom.getsend(this.dir);		this.mom.undrawWoods(this.dir);	}			function Path (){		var twn : Tween = new Tween (this.arrow, "_alpha", Regular.easeInOut, this.arrow._alpha, 0, 2, true);		//this.arrow._alpha = 0;	}}