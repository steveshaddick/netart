class Toppermost extends Trelement{		function Toppermost(_biasDir:String, _cityScore:Number, _popScore:Number, _count:Number){		var movName:String;						if (_biasDir)				this.side = _side;		this.travelled = travelled;				switch(this.side){			case "left":			movName = "topleft";			distance = 300;			break;						case "right":			movName = "topright";			distance = -300;			break;					}						this.initTrelement(movName, 10000+_num, distance);					}}