class Bush extends Trelement{		function Bush(_side:String, _travelled:Number, _num:Number){		var movName:String;		var distance:Number;		var torb:String;		var rnd:String;				if(this.randRange(1,100) < 75){			torb = "tree";			rnd = String(this.randRange(1,5));		} else {			torb = "bush";			rnd = "";		}				this.side = _side;		this.travelled = travelled;				switch(this.side){			case "left":			movName = torb + rnd;			distance = this.randRange(0, 350);			break;						case "right":			movName = torb + rnd;			distance = -this.randRange(0, 350);			break;					}		this.initTrelement(movName, 1000+_num, distance);	}}