import mx.transitions.Tween;import mx.transitions.easing. *;class Brush extends MovieClip{		public var dir:String;	public var mom:MovieClip;		private var arrow : MovieClip;	private var toobrushy:MovieClip;			function onRollOver (){		var twn : Tween = new Tween (this, "_x", Elastic.easeOut, this._x, this._x+Math.round(Math.random()*15), 0.5, true);	}			function onRollOut (){		var twn : Tween = new Tween (this, "_x", Elastic.easeOut, this._x, this._x-Math.round(Math.random()*15), 0.5, true);	}		function onRelease(){		//trace("brush release");		this.toobrushy = this.mom.attachMovie("linePoem", "toobrushy", 1000002, {_x:200, _y:Stage.height - 60});		this.toobrushy.txtLine.text = "The brush is too thick.";			}			function Brush(){	}}